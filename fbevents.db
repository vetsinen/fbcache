u0435\u0439\u0447\u0430\u0441 \u044d\u0442\u0438\u043c \u0436\u0438\u0432\u0435\u043c. \u041c\u044b \u043d\u0435 \u043f\u043e\u043b\u0443\u0447\u0430\u043b\u0438 \u043b\u0438\u0446\u0435\u043d\u0437\u0438\u0439, \u043d\u0435 \u043f\u0440\u043e\u0445\u043e\u0434\u0438\u043b\u0438 \u043e\u0431\u0443\u0447\u0435\u043d\u0438\u0435 \u0443 \u0442\u0435\u043e\u0440\u0435\u0442\u0438\u043a\u043e\u0432 \u043d\u0430 \u043f\u0440\u0438\u043c\u0435\u0440\u0435 Zappos. \u041c\u044b \u043f\u0440\u0438\u0448\u043b\u0438 \u043a \u043d\u0435\u0439 \u0432 \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u0435 \u0441\u0432\u043e\u0435\u0433\u043e \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u044f, \u043f\u043e\u0442\u043e\u043c\u0443 \u043d\u0435 \u0443\u0447\u0438\u043c \u0425\u043e\u043b\u0430\u043a\u0440\u0430\u0442\u0438\u0438, \u043c\u044b \u043e\u043f\u0438\u0440\u0430\u0435\u043c\u0441\u044f \u043d\u0430 \u043d\u0435\u0435 \u0438 \u0434\u0430\u0435\u043c \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u043f\u0440\u043e\u0434\u0443\u043a\u0442 \u043c\u0435\u043d\u0435\u0434\u0436\u043c\u0435\u043d\u0442\u0430:\n\n2009. White Sales (\u0442\u043e\u0433\u0434\u0430 Mark&Sales) \u043d\u0430\u0447\u0430\u043b\u0430 \u043f\u0440\u0435\u0434\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u0442\u044c \u0443\u0441\u043b\u0443\u0433\u0438 \u0440\u0435\u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0446\u0438\u0438 \u043f\u0440\u043e\u0434\u0430\u0436 \u0434\u043b\u044f \u0431\u0438\u0437\u043d\u0435\u0441\u0430.\n2010. \u0420\u043e\u0434\u0438\u043b\u0430\u0441\u044c \u0444\u0438\u043b\u043e\u0441\u043e\u0444\u0438\u044f \u201c\u0431\u0435\u043b\u044b\u0445 \u043f\u0440\u043e\u0434\u0430\u0436\u201d (\u041f\u0440\u043e\u0434\u0430\u0436\u0438 \u0434\u043e\u043b\u0436\u043d\u044b \u043f\u0440\u0438\u043d\u043e\u0441\u0438\u0442\u044c \u0443\u0434\u043e\u0432\u043e\u043b\u044c\u0441\u0442\u0432\u0438\u0435 \u0438 \u043a\u043b\u0438\u0435\u043d\u0442\u0443, \u0438 \u043f\u0440\u043e\u0434\u0430\u0432\u0446\u0443). \u041e\u043d\u0430 \u0437\u0430\u043f\u0440\u0435\u0449\u0430\u043b\u0430 \u0445\u043e\u043b\u043e\u0434\u043d\u044b\u0435 \u0437\u0432\u043e\u043d\u043a\u0438, \u0441\u043f\u0430\u043c, \u0436\u0435\u0441\u0442\u043a\u0438\u0435 \u043f\u0435\u0440\u0435\u0433\u043e\u0432\u043e\u0440\u044b, \u043e\u0442\u043a\u0430\u0442\u044b \u0438 \u0434\u0440.\n2011. \u041e\u0442\u043a\u0440\u044b\u043b\u0430\u0441\u044c \u0448\u043a\u043e\u043b\u0430 \u043f\u0440\u043e\u0434\u0430\u0436 White Sales School, \u0432 \u043a\u043e\u0442\u043e\u0440\u043e\u0439 \u043e\u043f\u044b\u0442 \u043f\u0440\u0435\u043f\u043e\u0434\u0430\u0432\u0430\u0442\u0435\u043b\u0435\u0439 \u0438 \u043d\u0430\u0448\u0435\u0439 \u043a\u043e\u043c\u0430\u043d\u0434\u044b \u0431\u044b\u043b\u0438 \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0438\u0440\u043e\u0432\u0430\u043d\u044b \u0432 \u043a\u043e\u043d\u043a\u0440\u0435\u0442\u043d\u044b\u0435 \u0430\u043b\u0433\u043e\u0440\u0438\u0442\u043c\u044b.\n2012. \u041d\u0430\u0447\u0430\u043b\u0438 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u0438\u0437\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u043f\u0440\u043e\u0434\u0430\u0436\u0438: CRM, sip-\u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0438\u044f, \u043e\u0431\u043b\u0430\u0447\u043d\u044b\u0435 \u0441\u0435\u0440\u0432\u0438\u0441\u044b.\n2014. \u0418\u0437\u0443\u0447\u0438\u043b\u0438 \u0441\u0438\u0441\u0442\u0435\u043c\u0443 \u201c\u041a\u0430\u043d\u0431\u0430\u043d\u201d \u0434\u043b\u044f \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0438 \u043d\u0430 \u0435\u0433\u043e \u043e\u0441\u043d\u043e\u0432\u0435 \u0430\u043b\u0433\u043e\u0440\u0438\u0442\u043c\u0430 \u043f\u0440\u043e\u0434\u0430\u0436 \u2014 \u043a\u043e\u043d\u0432\u0435\u0439\u0435\u0440.\n2015. \u0412\u0432\u0435\u043b\u0438 \u0441\u0432\u043e\u0431\u043e\u0434\u043d\u044b\u0439 \u0433\u0440\u0430\u0444\u0438\u043a, \u043f\u0435\u0440\u0435\u0448\u043b\u0438 \u043e\u0442 \u0441\u0442\u0430\u0432\u043e\u043a \u043a % \u043e\u0442 \u0434\u043e\u0445\u043e\u0434\u0430 \u0434\u043b\u044f \u0432\u0441\u0435\u0439 \u043a\u043e\u043c\u0430\u043d\u0434\u044b \u0438 \u043e\u0442\u043a\u0430\u0437\u0430\u043b\u0438\u0441\u044c \u043e\u0442 \u0438\u0435\u0440\u0430\u0440\u0445\u0438\u0438.\n10 \u043b\u0435\u0442. \u0412\u0441\u0435 \u044d\u0442\u043e \u0432\u0440\u0435\u043c\u044f \u043c\u044b \u0441\u043e\u0437\u0434\u0430\u0432\u0430\u043b\u0438 \u0438 \u043e\u0431\u043d\u043e\u0432\u043b\u044f\u043b\u0438 \u043a\u043e\u043c\u0430\u043d\u0434\u044b \u043f\u0440\u043e\u0434\u0430\u0436, \u0432\u043d\u0435\u0434\u0440\u044f\u044f \u0432 \u201c\u043a\u0440\u0430\u0441\u043d\u044b\u0435\u201d \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0438 \u2014 \u201c\u0437\u0435\u043b\u0435\u043d\u044b\u0435\u201d \u0438 \u201c\u0436\u0435\u043b\u0442\u044b\u0435\u201d \u043e\u0442\u0434\u0435\u043b\u044b \u043f\u0440\u043e\u0434\u0430\u0436 \u0432 \u0431\u043e\u043b\u0435\u0435 \u0447\u0435\u043c 100 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u044f\u0445. \n\n\u0418\u043c\u0435\u043d\u043d\u043e \u043f\u043e\u044d\u0442\u043e\u043c\u0443 \u0432\u0441\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0430 \u043f\u043e\u0441\u0442\u0440\u043e\u0435\u043d\u0430 \u043d\u0430 \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u043c \u043e\u043f\u044b\u0442\u0435 \u0441\u043e\u0437\u0434\u0430\u0442\u0435\u043b\u044f \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0438, \u043a\u0435\u0439\u0441\u0430\u0445 \u0435\u0433\u043e \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432 \u0438 \u0441\u0442\u0443\u0434\u0435\u043d\u0442\u043e\u0432 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u044b.\n\n\u041f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0430:\n\n1. \u0425\u043e\u043b\u0430\u043a\u0440\u0430\u0442\u0438\u044f \u043f\u0440\u043e\u0434\u0430\u0436\na. \u0425\u043e\u043b\u0430\u043a\u0440\u0430\u0442\u0438\u044f \u0411\u0440\u0430\u0439\u0430\u043d\u0430 \u0414\u0436. \u0420\u043e\u0431\u0435\u0440\u0442\u0441\u043e\u043d\u0430. \u0422\u0435\u043e\u0440\u0438\u044f, \u043f\u0440\u0430\u043a\u0442\u0438\u043a\u0430 \u0438 \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u044b.\nb. \u0420\u0430\u0431\u0441\u0442\u0432\u043e? \u041f\u043e\u0447\u0435\u043c\u0443 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0438 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u044e\u0442 \u043c\u0435\u043d\u0435\u0434\u0436\u043c\u0435\u043d\u0442 \u0440\u043e\u0434\u043e\u043c \u0438\u0437 \u0414\u0440\u0435\u0432\u043d\u0435\u0433\u043e \u0415\u0433\u0438\u043f\u0442\u0430? \u0418\u0435\u0440\u0430\u0440\u0445\u0438\u044f, \u043f\u043e\u0447\u0430\u0441\u043e\u0432\u043a\u0430, \u0448\u0442\u0440\u0430\u0444\u044b, \u0441\u0442\u0430\u0432\u043a\u0438, \u0442\u0440\u0435\u043d\u0438\u043d\u0433\u0438, \u043a\u043e\u0440\u043f\u043e\u0440\u0430\u0442\u0438\u0432\u044b.\nc. \u042d\u0432\u043e\u043b\u044e\u0446\u0438\u044f? \u0421\u0442\u0430\u0434\u0438\u0438 \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u044f \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0446\u0438\u0439 - \u0437\u0430\u0447\u0435\u043c \u0438 \u043a\u0430\u043a \u0440\u0430\u0437\u0432\u0438\u0432\u0430\u0442\u044c \u043a\u043e\u043c\u0430\u043d\u0434\u0443 \u0432 \u0441\u0432\u043e\u0435\u0439 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0438?\nd. \u041f\u043e\u0434\u0441\u043e\u0437\u043d\u0430\u043d\u0438\u0435. \u041f\u043e\u0447\u0435\u043c\u0443 \u0441\u0438\u0441\u0442\u0435\u043c\u0430 \u0438 \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u0430 \u0432\u0430\u0436\u043d\u0435\u0435 \u0437\u043d\u0430\u043d\u0438\u0439 \u0438 \u043e\u043f\u044b\u0442\u0430? \n\n2. \u0421\u0438\u0441\u0442\u0435\u043c\u0430\na. \u0421\u043e\u0446\u0438\u0430\u043b\u044c\u043d\u044b\u0439 \u043a\u043e\u043d\u0442\u0440\u0430\u043a\u0442. \u041a\u043e\u043d\u0441\u0442\u0438\u0442\u0443\u0446\u0438\u044f, \u0437\u0430\u043f\u043e\u0432\u0435\u0434\u0438, \u043f\u0440\u0430\u0432\u0438\u043b\u0430, \u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0442\u044b, \u0441\u043f\u043e\u0441\u043e\u0431\u043d\u044b\u0435 \u0437\u0430\u043c\u0435\u043d\u0438\u0442\u044c \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044f. \nb. \u041a\u043e\u043d\u0432\u0435\u0439\u0435\u0440. \u0410\u043b\u0433\u043e\u0440\u0438\u0442\u043c \u043f\u043e\u0441\u0442\u0440\u043e\u0435\u043d\u0438\u044f \u043f\u0440\u043e\u0434\u0430\u0436 \u0432 \u0441\u0438\u0441\u0442\u0435\u043c\u0435 \u041a\u0430\u043d\u0431\u0430\u043d \u2014 \u043f\u0443\u0442\u044c \u043a\u043b\u0438\u0435\u043d\u0442\u0430 \u0447\u0435\u0440\u0435\u0437 \u0432\u0441\u044e \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u044e (\u0432 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0438 \u043f\u0440\u043e\u0434\u0430\u044e\u0442 \u0432\u0441\u0435).\nc. Sales Book. \u0427\u0442\u043e \u0438 \u043a\u0430\u043a \u0434\u0435\u043b\u0430\u0442\u044c \u043f\u0440\u0438 \u043b\u044e\u0431\u043e\u043c \u0440\u0443\u0442\u0438\u043d\u043d\u043e\u043c \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0438: \u044d\u0442\u0430\u043f\u044b, \u0447\u0435\u043a-\u043b\u0438\u0441\u0442\u044b \u0438 \u0441\u043a\u0440\u0438\u043f\u0442\u044b. \u0420\u0430\u0437\u0434\u0435\u043b\u0435\u043d\u0438\u0435 \u0441\u043b\u043e\u0436\u043d\u043e\u0433\u043e \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u0430 \u043d\u0430 \u043f\u0440\u043e\u0441\u0442\u044b\u0435 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u044f. \n\n3. \u041b\u044e\u0434\u0438\na. \u0421\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0430. \u0427\u0435\u043c \u0437\u0430\u043c\u0435\u043d\u0438\u0442\u044c \u0438\u0435\u0440\u0430\u0440\u0445\u0438\u044e, \u043a\u0430\u043a\u0438\u0435 \u043d\u0443\u0436\u043d\u044b \u0440\u043e\u043b\u0438 \u0438 \u043a\u0430\u043a \u0440\u0430\u0437\u0434\u0435\u043b\u0438\u0442\u044c \u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0441\u0442\u044c?\nb. \u041c\u043e\u0442\u0438\u0432\u0430\u0446\u0438\u044f. \u041a\u043e\u043d\u043a\u0440\u0435\u0442\u043d\u044b\u0435 \u0446\u0435\u043b\u0438 (\u0431\u0435\u0437 \u043f\u043b\u0430\u043d\u043e\u0432), \u0441\u043f\u0440\u0430\u0432\u0435\u0434\u043b\u0438\u0432\u0430\u044f \u043e\u043f\u043b\u0430\u0442\u0430 (\u0431\u0435\u0437 \u0441\u0442\u0430\u0432\u043a\u0438) \u0438 \u043f\u0440\u043e\u0437\u0440\u0430\u0447\u043d\u044b\u0439 \u043c\u0435\u043d\u0435\u0434\u0436\u043c\u0435\u043d\u0442 (\u0431\u0435\u0437 \u043e\u0431\u043c\u0430\u043d\u0430).\nc. \u041c\u0435\u043d\u0442\u043e\u0440\u0441\u0442\u0432\u043e. \u041a\u043e\u043d\u0442\u0440\u043e\u043b\u044c \u0441\u0438\u0441\u0442\u0435\u043c\u044b, \u0438\u0441\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u0435 \u0431\u0430\u0433\u043e\u0432 \u0441\u0438\u0441\u0442\u0435\u043c\u044b \u0438 \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u0435 \u0438\u043d\u0434\u0438\u0432\u0438\u0434\u0443\u0430\u043b\u044c\u043d\u044b\u0445 \u043d\u0430\u0432\u044b\u043a\u043e\u0432 \u043a\u043e\u043c\u0430\u043d\u0434\u044b.\n\n4. \u041c\u0430\u0448\u0438\u043d\u044b\na. CRM. \u0412\u044b\u0431\u043e\u0440 \u0438 \u0432\u043d\u0435\u0434\u0440\u0435\u043d\u0438\u0435 CRM. \u0421\u0445\u0435\u043c\u044b, \u0442\u0435\u0445\u043d\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u0437\u0430\u0434\u0430\u043d\u0438\u044f \u0438 \u0430\u043b\u0433\u043e\u0440\u0438\u0442\u043c \u0437\u0430\u043f\u0443\u0441\u043a\u0430.\nb. \u041a\u043e\u043c\u043c\u0443\u043d\u0438\u043a\u0430\u0446\u0438\u044f. \u0412\u0441\u0435 \u043d\u043e\u0432\u044b\u0435 \u043e\u0431\u0440\u0430\u0449\u0435\u043d\u0438\u044f (\u0442\u0435\u043b\u0435\u0444\u043e\u043d, \u043c\u0435\u0441\u0441\u0435\u043d\u0434\u0436\u0435\u0440\u044b, email, \u0444\u043e\u0440\u043c\u044b) \u043f\u043e\u043f\u0430\u0434\u0430\u044e\u0442 \u043d\u0430 \u041a\u043e\u043d\u0432\u0435\u0439\u0435\u0440 (\u0432 CRM), \u0432\u0441\u0435 \u043f\u043e\u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u0435 \u2014 \u043f\u0440\u0438\u043a\u